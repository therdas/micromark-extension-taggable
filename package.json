{
  "name": "micromark-extension-taggable",
  "version": "0.1.1",
  "description": "Parse and Render custom #tags and @mentions",
  "keywords": [
    "remark",
    "remark-plugin",
    "markdown",
    "efm",
    "micromark",
    "micromark-plugin"
  ],
  "author": {
    "name": "Rahul Das",
    "email": "contact@therdas.dev",
    "url": "https://www.therdas.dev"
  },
  "license": "MIT",
  "repository": "https://github.com/therdas/micromark-extension-taggable",
  "type": "module",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "build": "rollup -c",
    "lint": "eslint src/",
    "prepare": "npm run build",
    "pretest": "npm run build",
    "format": "npx prettier . --write",
    "test": "npm run lint && npm run format && node --experimental-vm-modules node_modules/jest/bin/jest.js"
  },
  "jest": {
    "collectCoverage": true,
    "coveragePathIgnorePatterns": [
      "node_modules",
      "<rootDir>/src/index.d.ts"
    ],
    "collectCoverageFrom": [
      "./src/**"
    ],
    "coverageThreshold": {
      "global": {
        "lines": 90
      }
    }
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.25.9",
    "@babel/plugin-transform-typescript": "^7.25.9",
    "@babel/preset-env": "^7.26.0",
    "@eslint/js": "^9.16.0",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^28.0.1",
    "@rollup/plugin-node-resolve": "^15.3.0",
    "c8": "^10.1.2",
    "eslint": "^9.16.0",
    "globals": "^15.13.0",
    "jest": "^29.7.0",
    "micromark": "^4.0.1",
    "micromark-util-character": "^2.1.1",
    "micromark-util-symbol": "^2.0.1",
    "micromark-util-types": "^2.0.1",
    "prettier": "^3.4.1",
    "rollup": "^4.28.0",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-dts": "^6.1.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-typescript2": "^0.36.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2"
  }
}
